	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
        String s1=sc.nextLine();
        char[] ch=s1.toCharArray(); 
        char[] Ja = new char[5];
        for(int i=s1.length();i>=0;i--)
        {
        	
        	System.out.println(Ja[i]);
        }
	}

}
