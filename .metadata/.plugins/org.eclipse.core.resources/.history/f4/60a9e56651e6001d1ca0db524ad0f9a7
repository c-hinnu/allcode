package javanew;

import java.util.Arrays;

public class sample {

	public static void main(String[] args) {
	String s="chninnu";
	char[] a=s.toCharArray();
	Arrays.sort(a);
	int count=0;
	int i,j;
	for(i=0;i<a.length-1;i++) 
	{
		
			
		if(a[i]==a[i+1])
		{
			count=count+1;
			System.out.print(a[i]);
			System.out.print(count+1);
			
		}
		if(count==0)
		{
			System.out.print(a[i]);
		}
	}
	
	System.out.print(a[i]);
	
}
}
