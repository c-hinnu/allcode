package javanew;
import java.util.*;

public class pgm17 {

	public static void main(String[] args) {
		   Scanner sc = new Scanner(System.in);
		     int n = sc.nextInt();
		     if(n<5)
		     {
		         for(int i=0;i<n;i++)
		         {
		             System.out.println("Number= "+i);
		         }
		     }
		     else
		     {
		         System.out.println("Entered Number > 5, re-enter");
		     }
		    }
		}
