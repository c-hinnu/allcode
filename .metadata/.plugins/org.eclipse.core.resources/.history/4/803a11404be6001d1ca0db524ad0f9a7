package javanew;

public class sample {

	public static void main(String[] args) {
	String s="chinnu";
	char[] a=s.toCharArray();
	int count=0;
	for(int i=0;i<a.length;i++) {
		if(count==0)
		{
			System.out.print(a[i]);
		}
	
		if(a[i]==a[i+1])
		{
			count=count+1;
			System.out.print(a[i]);
			
		}
		System.out.println(count);
	}

}}
