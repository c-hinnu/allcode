package javanew;
import java.util.*;

public class pgm16 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
        int sum=0;
        
        if(n>=5)
        {
         for(int i=0;i<n;i++)   
         {
             int rem=i%10;
             sum=sum+rem;
             i=i/10;
             if(sum==5)
             {
                 System.out.print(sum);
                 break;
             }
         }
        }
        else
        {
            System.out.println("invalid");
        }
    }
    
}