package javanew;

public class sample {

	public static void main(String[] args) {
	String s="chninnu";
	char[] a=s.toCharArray();
	int count=0;
	int i;
	for(i=0;i<a.length-1;i++) 
	{
		for(int j=i+1;j<a.length-1;j++)
		{
			
		if(a[i]==a[j])
		{
			count=count+1;
			System.out.print(a[i]);
			System.out.print(count+1);
			
		}
		}
		
		if(count==0)
		{
			System.out.print(a[i]);
		}
		
	}
	
	System.out.print(a[i]);

}}
