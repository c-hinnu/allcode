package javanew;
import java.util.*;

public class Arrmax {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n= sc.nextInt();
		int[] lar=new int[n];
		
		for(int i=0;i<n;i++)
		{
			lar[i]=sc.nextInt();
		}
	    Arrays.sort(lar);
	    
	    for(int i=0;i<n;i++)
		{
			System.out.println(lar[i]);
		}
	    
	    System.out.println(lar[n-2]);
		

	}

}
